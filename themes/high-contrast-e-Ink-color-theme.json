{
	"name": "High Contrast E Ink",
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"colors": {
		"activityBar.background": "#ffffff",
		"activityBar.foreground": "#000000",
		"activityBar.inactiveForeground": "#000000",
		"activityBarBadge.background": "#ffffff",
		"activityBarBadge.foreground": "#000000",
		"badge.background": "#ffffff",
		"badge.foreground": "#000000",
		"button.background": "#ffffff",
		"button.foreground": "#000000",
		"button.hoverBackground": "#ffffff",
		"checkbox.border": "#000000",
		"contrastActiveBorder": "#000000",
		"contrastBorder": "#000000",
		"dropdown.border": "#000000",
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editor.selectionBackground": "#999999",
		"editor.selectionHighlightBackground": "#cccccc",
		"editorGroup.border": "#000000",
		"editorGroup.dropBackground": "#0000007a",
		"editorGroupHeader.tabsBorder": "#000000",
		"extensionButton.prominentBackground": "#ffffff",
		"extensionButton.prominentForeground": "#000000",
		"extensionButton.prominentHoverBackground": "#ffffff",
		"focusBorder": "#000000",
		"foreground": "#000000",
		"icon.foreground": "#ffffff",
		"input.border": "#000000",
		"inputOption.activeBorder": "#000000",
		"menu.border": "#ffffff",
		"menu.selectionBackground": "#ffffff",
		"menu.selectionBorder": "#000000",
		"menu.selectionForeground": "#000000",
		"menubar.selectionBorder": "#000000",
		"panel.border": "#000000",
		"panelInput.border": "#000000",
		"selection.background": "#999999",
		"sideBarSectionHeader.background": "#ffffff",
		"sideBarSectionHeader.border": "#000000",
		"statusBar.background": "#ffffff",
		"statusBar.debuggingBackground": "#ffffff",
		"statusBar.foreground": "#000000",
		"statusBar.noFolderBackground": "#ffffff",
		"tab.border": "#000000",
		"titleBar.activeBackground": "#ffffff",
		"titleBar.activeForeground": "#000000",
		"titleBar.border": "#000000",
		"titleBar.inactiveBackground": "#ffffff",
		"titleBar.inactiveForeground": "#000000",
		"widget.shadow": "#000000",
		"window.activeBorder": "#000000",
		"window.inactiveBorder": "#000000",
	},
	"tokenColors": [
		{
			"scope": "support.function, variable.function",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.function-call entity.name.function",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.other.object.key string.unquoted",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "string.quoted, string, punctuation.tag.definition.string, tag.string",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#888888",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}