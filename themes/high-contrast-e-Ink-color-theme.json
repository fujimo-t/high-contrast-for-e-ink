	"tokenColors": [
		{
			"scope": "support.function, variable.function",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.function-call entity.name.function",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.other.object.key string.unquoted",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "string.quoted, string, punctuation.tag.definition.string, tag.string",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#888888",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}